<h1>Loan Request</h1>
<table>
  <tr>
    <td>
      <label>
        Name:<br/>
        <input type="text" name="name" value="" />
        <input type="hidden" name="name_required" value="true" />
        <input type="hidden" name="name_type" value="String" />
      </label><br/>
    </td>
  </tr>
  <tr>
    <td>
      <label>
        Email address:<br/>
        <input type="text" name="emailAddress" value="" />
        <input type="hidden" name="emailAddress_required" value="true" />
        <input type="hidden" name="emailAddress_type" value="String" />
      </label><br/>
    </td>
  </tr>
  <tr>
    <td>
      <label>
        Income:<br/>
        <input type="text" name="income" value="" />
        <input type="hidden" name="income_required" value="true" />
        <input type="hidden" name="income_type" value="Integer" />
      </label><br/>
    </td>
  </tr>
  <tr>
    <td>
      <label>
        Requested loan amount:<br/>
        <input type="text" name="loanAmount" value="" />
        <input type="hidden" name="loanAmount_required" value="true" />
        <input type="hidden" name="loanAmount_type" value="Integer" />
      </label><br/>
    </td>
  </tr>
</table>
